// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id        Int       @id @default(autoincrement())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Fake {
  id             Int    @id @default(autoincrement())
  theme          String @default("")
  subtheme       String @default("")
  discription    String @default("")
  verdict        String @default("")
  source         String @default("")
  date           String @default("")
  link           String @default("")
  country        String @default("")
  disproof       String @default("")
  tags           String @default("")
  vox_article_id Int?
  video_id       String @default("")
  infographic_id String @default("")
}

model Article {
  id                     Int    @id @default(autoincrement())
  vox_article_title      String @default("")
  vox_article_link       String @default("")
  vox_article_image_link String @default("")
}

model Video {
  id              Int    @id @default(autoincrement())
  vox_video_title String @default("")
  vox_video_link  String @default("")
}

model Infographic {
  id                    Int    @id @default(autoincrement())
  vox_infographic_title String @default("")
  vox_infographic_link  String @default("")
}
